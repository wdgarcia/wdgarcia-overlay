<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
  <_name>Upgrade</_name>
  <id>sozi.extras.upgrade</id>
  <dependency type="executable" location="extensions">inkex.py</dependency>
  <dependency type="executable" location="extensions">sozi_extras_upgrade.py</dependency>
  <effect>
    <object-type>all</object-type>
    <effects-menu>
      <submenu _name="Sozi extras"/>
    </effects-menu>
  </effect>
  <script>
    <command reldir="extensions" interpreter="python">sozi_extras_upgrade.py</command>
  </script>
</inkscape-extension>


